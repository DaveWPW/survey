<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dave.dao.PaperQuesDao">
<!-- 
PAPER_QUES_ID	NUMBER(10,0)	No		1	
PAPER_ID	NUMBER(10,0)	Yes		2	
QUES_NUM	NUMBER(10,0)	Yes		3	
QUES_ID	NUMBER(10,0)	Yes		4	
PARENT_OPTION_ID	NUMBER(10,0)	Yes		5	父级选项ID
 -->
	<insert id="addPaperQues" parameterType="com.dave.entity.PaperQues">
     	insert into su_paper_ques
      	(paper_id, ques_num, ques_id)
      	values
		(#{paperId}, #{quesNum}, #{quesId})
	</insert>
</mapper>