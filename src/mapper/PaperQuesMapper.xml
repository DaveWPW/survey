<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dave.dao.PaperQuesDao">
	<insert id="addPaperQues" parameterType="com.dave.entity.PaperQues">
     	insert into su_paper_ques
      	(paper_id, ques_num, ques_id)
      	values
		(#{paperId}, #{quesNum}, #{quesId})
	</insert>
</mapper>